<!DOCTYPE mapper     
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="statics.dao.testUserDao">
	<insert id="insertUser" parameterType="testUser">
		insert into test_user2
			   (email, pass)
		values (#{email}, #{pass})
	</insert>
</mapper>